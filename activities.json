    id: "291",
    name: "Plaza Mayor ",
    img: "https://madrid-shops.com/media/shops/plaza-small.jpg",
    logo_img: "https://madrid-shops.com/media/shops/logo-plaza-200.jpg",
    opening_hours_en: "-",
